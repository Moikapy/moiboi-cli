{
  "name": "boilerplate-cli",
  "version": "0.1.0",
  "description":
    "CLI to search and download everyone's boilerplates on github.",
  "bin": {
    "boil": "src/index.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=9.5.0"
  },
  "keywords": [
    "boil",
    "boilerplate",
    "cli",
    "github",
    "search",
    "create",
    "download",
    "template",
    "generator"
  ],
  "repository": "git@github.com:keidrun/boilerplate-cli.git",
  "author": "keidrun",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "opener": "^1.4.3",
    "readline-sync": "^1.4.9",
    "rimraf": "^2.6.2",
    "yargs": "^11.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "mocha",
    "watch": "mocha --watch"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "nock": "^9.2.2",
    "sinon": "^4.4.2"
  }
}
