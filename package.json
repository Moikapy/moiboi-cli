{
	"name": "moiboi-cli",
	"version": "0.2.1",
	"description": "CLI making Git Clone Simplified",
	"bin": {
		"moiboi": "dist/index.js"
	},
	"preferGlobal": true,
	"keywords": [
		"moiboi",
		"repository",
		"boilerplate",
		"cli",
		"github",
		"search",
		"create",
		"download",
		"template",
		"generator"
	],
	"repository": "https://github.com/Moikapy/moiboi-cli.git",
	"author": "Moikapy",
	"license": "MIT",
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"axios": "^0.19.0",
		"opener": "^1.5.1",
		"readline-sync": "^1.4.9",
		"rimraf": "^2.6.3",
		"yargs": "^13.2.4"
	},
	"scripts": {
		"dev": "babel-node src/index.js",
		"build": "babel src -d dist",
		"start": "node dist/index.js",
		"test": "mocha --require @babel/register",
		"watch": "mocha --watch",
		"format": "prettier --single-quote --write './{src}/**/*.js'"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.0.0",
		"@babel/node": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"chai": "^4.2.0",
		"mocha": "^6.1.4",
		"nock": "^10.0.6",
		"prettier": "^1.17.1",
		"sinon": "^7.3.2"
	}
}
